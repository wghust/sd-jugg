vender(prop, args)
  -webkit-{prop} args
  -moz-{prop} args
  {prop} args

border-radius()
  vender('border-radius', arguments)

box-shadow()
  vender('box-shadow', arguments)

opacity(n)
  opacity n
  if support-for-ie
    filter unquote('progid:DXImageTransform.Microsoft.Alpha(Opacity=' + round(n * 100) + ')')

transition()
  vender('transition', arguments)

transform()
  vender('transform', arguments)

box-sizing()
  vender('box-sizing', arguments)
  
animation() 
  vender('animation', arguments)
  
body
  padding 0
  margin 0
  font-family -apple-system,".SFNSText-Regular","Helvetica Neue","Hiragino Sans GB","WenQuanYi Micro Hei","Microsoft Yahei",sans-serif,Meiryo
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  
.table-wrap
  overflow-x auto
  th,td
    white-space nowrap
  .tb-table thead>tr>th,.tb-table tbody>tr>td
    vertical-align middle

.container
    width calc(100% - 220px)
    margin-left 210px
    padding-top 0px
    min-height 500px
    transition all 0.25s
  .fullContainer
    width calc(100% - 30px)
    margin-left 15px
  
